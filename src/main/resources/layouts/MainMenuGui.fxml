<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<Scene xmlns="http://javafx.com/javafx/11.0.1"  xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane prefHeight="200" prefWidth="200">
      <children>
         <VBox alignment="CENTER" layoutX="50.0" prefHeight="200.0" prefWidth="100.0">
            <children>
               <Button id="startButton" mnemonicParsing="false" text="Start" />
               <Button id="exitButton" mnemonicParsing="false" text="Exit">
                  <VBox.margin>
                     <Insets top="40.0" />
                  </VBox.margin>
               </Button>
            </children>
         </VBox>
      </children></AnchorPane>
</Scene>
