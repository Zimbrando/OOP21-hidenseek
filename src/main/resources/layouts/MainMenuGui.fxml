<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane fx:id="gamedgsdfsdf" xmlns="http://javafx.com/javafx/11.0.1"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="hidenseek.controller.MainMenuController">
    <AnchorPane styleClass="rootStyle" fx:id="mainPane" prefWidth="1920" prefHeight="1080">  
      <children>
         <VBox alignment="CENTER" layoutX="50.0" prefHeight="200.0" prefWidth="100.0">
            <children>
               <Button fx:id="startButton" mnemonicParsing="false" text="Start" onAction="#handleStartClicked">
               </Button>
               <Button fx:id="exitButton" mnemonicParsing="false" text="Exit" onAction="#handleExitClicked">
                  <VBox.margin>
                     <Insets top="40.0" />
                  </VBox.margin>
               </Button>
            </children>
         </VBox>
      </children></AnchorPane>
</Pane>
